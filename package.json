  "scripts": {
    "build": "nwb build-react-component && npm run postbuild",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "tests/dummy.sh",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "postinstall": "echo 'done'",
    "postbuild": "cp src/classes/labels.json ./es/classes/ && mkdir -p es/images/graphics/flags && mkdir -p lib/images/graphics/flags && cp -R src/images/graphics ./es/images/ && cp -R 'src/images/graphics' ./lib/images/"
  },
